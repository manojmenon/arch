{
  "name": "veedu-project-management-mcp-servers",
  "version": "1.0.0",
  "description": "MCP servers for Veedu Project Management System APIs",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "node index.js",
    "dev": "node --watch index.js",
    "start:auth": "node auth/server.js",
    "start:organizations": "node organizations/server.js",
    "start:projects": "node projects/server.js",
    "start:users": "node users/server.js",
    "start:categories": "node categories/server.js",
    "start:tags": "node tags/server.js",
    "start:all": "concurrently \"npm run start:auth\" \"npm run start:organizations\" \"npm run start:projects\" \"npm run start:users\" \"npm run start:categories\" \"npm run start:tags\"",
    "test": "jest",
    "lint": "eslint .",
    "format": "prettier --write ."
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "api",
    "project-management",
    "real-estate"
  ],
  "author": "Veedu Project Management",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^0.4.0",
    "axios": "^1.6.0",
    "dotenv": "^16.3.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "prettier": "^3.0.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
